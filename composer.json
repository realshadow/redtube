{
    "name": "realshadow/redtube-api",
    "description": "PHP based API wrapper for Redtube HTTP API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Realshadow\\Redtube\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Realshadow\\Redtube\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "jms/serializer": "^1.7",
        "illuminate/support": "^8.3",
        "myclabs/php-enum": "^1.5",
        "guzzlehttp/guzzle": "^7.3"
    }
}
